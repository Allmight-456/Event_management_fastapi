# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/event_management

# Security Configuration
SECRET_KEY=your-super-secret-key-change-this-in-production-make-it-long-and-complex
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# Redis Configuration
REDIS_URL=redis://localhost:6379

# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=Event Management API
VERSION=1.0.0

# Rate Limiting
RATE_LIMIT_PER_MINUTE=100

# Environment
ENVIRONMENT=development
DEBUG=true
EOF

# Create .env.example for documentation (without sensitive values)
cat > .env.example << EOF
DATABASE_URL=postgresql://user:password@localhost:5432/event_management
SECRET_KEY=your-secret-key-here
REDIS_URL=redis://localhost:6379
API_V1_STR=/api/v1
PROJECT_NAME=Event Management API
VERSION=1.0.0
RATE_LIMIT_PER_MINUTE=100
ENVIRONMENT=development
DEBUG=true
EOF

# Create .gitignore to prevent sensitive files from being committed
cat > .gitignore << EOF
